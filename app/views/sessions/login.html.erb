<div class="center" align="center">
<h1>Log In Please</h1>



   <%= form_for @user, url: '/login' do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Log In" %>
    <br><br><button type="button"><%= link_to "Log In with Google", '/auth/google_oauth2' %></button>
    <% end %>
  

  
    </div>